{
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"target": "ES2022",
		"outDir": "dist",
		"noUnusedLocals": false,
		"lib": [
			"DOM"
		],
		"paths": {
			"@common/*": [
				"./common/*"
			],
			"@generated/*": [
				"./generated/*"
			],
			"@data/*": [
				"./data/*"
			],
			"~/*": [
				"./app/*"
			]
		},
		"resolveJsonModule": true,
		"jsx": "react-jsx",
		"esModuleInterop": true,
		"strictNullChecks": true,
	},
	"include": [
		"./app/**/*",
		"./market-crawler/**/*",
		"./common/**/*",
		"./generated/**/*",
		"./data/**/*",
		"./dev-utils/**/*"
	]
}